    <!-- @include 'nav.kit' -->
    <main class="main">
        <div class="hero-bg"></div>
        <section class="planet planet-mercury">
            <div class="wrapper">
                <div class="mobile-btns-panel mobile-btns-panel--mercury">
                    <button class="mobile-btns-panel__btn mobile-btns-panel__btn--active overview-btn">overview</button>
                    <button class="mobile-btns-panel__btn structure-btn">structure</button>
                    <button class="mobile-btns-panel__btn geology-btn">surface</button>
                    <div class="border"></div>
                </div>
                <div class="planet__imgs planet__imgs--mercury">
                    <img class="planet__imgs-img planet__imgs-img--first first-mercury" src="" alt="">
                    <img class="planet__imgs-img planet__imgs-img--second second-mercury" src="" alt="">
                    <img class="planet__imgs-img planet__imgs-img--popup popup-mercury" src="" alt="">
                </div>
                <div class="planet__main">
                    <div class="planet__info">
                        <h2 class="planet__info-title"></h2>
                        <p class="planet__info-text"></p>
                        <p class="planet__info-source">Source: <a href="" target="_blank"
                                rel="noopener noreferrer">Wikipedia</a> <img src="./dist/img/icons/icon-source.svg"
                                alt=""></p>
                    </div>
                    <div class="planet__btns-panel planet__btns-panel--mercury">
                        <button
                            class="planet__btns-panel-btn overview-btn planet__btns-panel-btn--mercury planet__btns-panel-btn--active"><span>01</span>overview</button>
                        <button
                            class="planet__btns-panel-btn structure-btn planet__btns-panel-btn--mercury"><span>02</span>Internal
                            structure</button>
                        <button
                            class="planet__btns-panel-btn geology-btn planet__btns-panel-btn--mercury"><span>03</span>surface
                            geology</button>
                    </div>
                </div>
            </div>
        </section>
